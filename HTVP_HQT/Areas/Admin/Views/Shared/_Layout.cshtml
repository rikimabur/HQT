<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <!-- Bootstrap -->
    <link href="~/Areas/Admin/assets/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="~/Areas/Admin/assets/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="~/Areas/Admin/assets/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="~/Areas/Admin/assets/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="~/Areas/Admin/assets/vendors/iCheck/skins/flat/green.css" rel="stylesheet">
    <!-- bootstrap-progressbar -->
    <link href="~/Areas/Admin/assets/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="~/Areas/Admin/assets/build/css/custom.min.css" rel="stylesheet">
    <link href="~/Areas/Admin/assets/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <!-- jQuery -->
    <script src="~/assets/jquery-2.2.1.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Areas/Admin/assets/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="~/Areas/Admin/assets/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    @RenderSection("css_ad", false)
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="javascript:;" class="site_title"><i class="fa fa-paw"></i> <span>Phước Con!</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- menu profile quick info -->
                    <div class="profile">
                        <div class="profile_pic">
                            <img src="~/hinhanh/admin.jpg" class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Xin Chào,</span>
                            <h2>Admin</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->
                    <br />
                    @*@Html.Partial("~/Areas/Admin/_sidebarMenu")
                    @Html.Partial("~/Areas/Admin/_sidebarFooter")*@
                </div>
            </div>
            @Html.Partial("~/Areas/Admin/_navigation")
            <!-- page content -->
            <div class="right_col" role="main">
                <div class="row">
                    <div class="page-title">
                        <div class="title_left">
                            <h3>@RenderSection("view_title_ad", true)</h3>
                        </div>
                    </div>
                </div>
                @RenderBody()
            </div>
            <!-- /page content -->
            @Html.Partial("~/Areas/Admin/_footer")
        </div>
    </div>
    @RenderSection("js_ad", false)
    <!-- FastClick -->
    <script src="~/Areas/Admin/assets/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Areas/Admin/assets/vendors/nprogress/nprogress.js"></script>
    <!-- iCheck -->
    <script src="~/Areas/Admin/assets/vendors/iCheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="~/Areas/Admin/assets/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="~/Areas/Admin/assets/vendors/Flot/jquery.flot.js"></script>
    <script src="~/Areas/Admin/assets/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/Areas/Admin/assets/vendors/Flot/jquery.flot.time.js"></script>
    <script src="~/Areas/Admin/assets/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="~/Areas/Admin/assets/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="~/Areas/Admin/assets/js_main/flot/jquery.flot.orderBars.js"></script>
    <script src="~/Areas/Admin/assets/js_main/flot/date.js"></script>
    <script src="~/Areas/Admin/assets/js_main/flot/jquery.flot.spline.js"></script>
    <script src="~/Areas/Admin/assets/js_main/flot/curvedLines.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="~/Areas/Admin/assets/js_main/moment/moment.min.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="~/Areas/Admin/assets/build/js/custom.min.js"></script>
    <!-- Flot -->
    <script>$(document).ready(function () {
            var data1 = [
              [gd(2012, 1, 1), 17],
              [gd(2012, 1, 2), 74],
              [gd(2012, 1, 3), 6],
              [gd(2012, 1, 4), 39],
              [gd(2012, 1, 5), 20],
              [gd(2012, 1, 6), 85],
              [gd(2012, 1, 7), 7]
            ];

            var data2 = [
              [gd(2012, 1, 1), 82],
              [gd(2012, 1, 2), 23],
              [gd(2012, 1, 3), 66],
              [gd(2012, 1, 4), 9],
              [gd(2012, 1, 5), 119],
              [gd(2012, 1, 6), 6],
              [gd(2012, 1, 7), 9]
            ];
            $("#canvas_dahs").length && $.plot($("#canvas_dahs"), [
              data1, data2
            ], {
                series: {
                    lines: {
                        show: false,
                        fill: true
                    },
                    splines: {
                        show: true,
                        tension: 0.4,
                        lineWidth: 1,
                        fill: 0.4
                    },
                    points: {
                        radius: 0,
                        show: true
                    },
                    shadowSize: 2
                },
                grid: {
                    verticalLines: true,
                    hoverable: true,
                    clickable: true,
                    tickColor: "#d5d5d5",
                    borderWidth: 1,
                    color: '#fff'
                },
                colors: ["rgba(38, 185, 154, 0.38)", "rgba(3, 88, 106, 0.38)"],
                xaxis: {
                    tickColor: "rgba(51, 51, 51, 0.06)",
                    mode: "time",
                    tickSize: [1, "day"],
                    //tickLength: 10,
                    axisLabel: "Date",
                    axisLabelUseCanvas: true,
                    axisLabelFontSizePixels: 12,
                    axisLabelFontFamily: 'Verdana, Arial',
                    axisLabelPadding: 10
                },
                yaxis: {
                    ticks: 8,
                    tickColor: "rgba(51, 51, 51, 0.06)",
                },
                tooltip: false
            });

            function gd(year, month, day) {
                return new Date(year, month - 1, day).getTime();
            }
        });</script>
    <!-- /Flot -->
    <!-- Skycons -->
    <script>$(document).ready(function () {
            var icons = new Skycons({
                "color": "#73879C"
            }),
              list = [
                "clear-day", "clear-night", "partly-cloudy-day",
                "partly-cloudy-night", "cloudy", "rain", "sleet", "snow", "wind",
                "fog"
              ],
              i;

            for (i = list.length; i--;)
                icons.set(list[i], list[i]);

            icons.play();
        });</script>
    <!-- /Skycons -->
</body>
</html>